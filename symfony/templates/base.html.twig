        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        <script src="{{ asset('libs/jquery/jquery-3.3.1.js') }}"></script>
        <script src="{{ asset('libs/popper/popper.js') }}"></script>
        <script src="{{ asset('libs/bootstrap/dist/js/bootstrap.js') }}"></script>
        {% block javascripts %}{% endblock %}
